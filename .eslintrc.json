{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "max-len": [2, 80],
    "no-param-reassign": [0],
    "no-shadow": [0],
    "no-use-before-define": [0],
    "strict": [2, "global"],
    "react/jsx-closing-bracket-location": [0],
    "no-else-return": [0],
    "space-before-function-paren": [2, "never"],
    "no-case-declarations": [0],
    "func-names": [0],
    "prefer-template": [0],
    "no-confusing-arrow": [0],
    "no-multi-spaces": [0],
    "new-cap": [0],
    "id-blacklist": [1,
      "overSome",
      "overEvery",
      "each",
      "eachRight",
      "extend",
      "extendWith"
    ]
  }
}
